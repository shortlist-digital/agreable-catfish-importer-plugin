{
  "name": "shortlist-digital/agreable-catfish-importer-plugin",
  "type": "wordpress-plugin",
  "description": "Wordpress plugin for Shortlist Media.",
  "license": "MIT",
  "repositories": [{
    "type": "package",
    "package": {
      "name": "jonsherrard/mesh",
      "version": "1.0",
      "dist": {
        "type": "zip",
        "url": "https://github.com/jonsherrard/mesh/archive/master.zip"
      }
    }
  }],
  "require": {
    "getherbert/framework": "~0.9",
    "facebook/webdriver": "~1.0",
    "behat/behat": "~2.5",
    "phpunit/phpunit": "5.0.*",
    "sunra/php-simple-html-dom-parser" : "~1.5",
    "maknz/slack": "^1.7",
    "sabre/xml": "^1.4",
    "jonsherrard/mesh": "*"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "AgreableCatfishImporterPlugin\\": "app/"
    }
  }
}
